  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ng launch",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "Start serve & debug",
      "url": "http://localhost:4200/*",
      "webRoot": "${workspaceFolder}/angular-app/src",
      "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/*"
      },
      "port": 9222,
    }
  ]
}